                    </identity>
                </endpoint>
                <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange" />
                <host>
                    <baseAddresses>
                        <add baseAddress="http://localhost:8733/Design_Time_Addresses/TestUI/chat_server/" />
                    </baseAddresses>
                </host>
            </service>
            <service name="TestUI.Service_Chat">
